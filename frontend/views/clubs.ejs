<script>
    function handleJoinButtonClick(button) {
        if (button.classList.contains('joined')) {
            button.innerText = 'Join';
            button.classList.remove('joined');
        } else {
            button.innerText = 'Joined';
            button.classList.add('joined');
        }
    }
</script>
