<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="/startupHome.css">
    <title>UpStart-StartUPs</title>
</head>
<body>
  <div id="google_translate_element"></div>
  <script type="text/javascript">
    function googleTranslateElementInit() {
      new google.translate.TranslateElement({pageLanguage: 'en'}, 'google_translate_element');
    }
    </script>    
    <nav class="navbar bg-body-tertiary">
        <div class="container-fluid">
          <a href="/postpage"><span class="navbar-brand mb-0 h1">UpStart</span></a>
        </div>
        <div class="container-fluid">
          <div id="google_translate_element"></div>
        </div>
      </nav>

    <div class="control">
      <a href="/addstartup">Register Startup</a>
    </div>
  
    <div class="searchBlock">
      <form action="/search" method="post">
        <div class="searchInput">
          <input name="searchele" type="text">
        </div>
        <button type="submit">Search</button>
      </form>
    </div>

   <div class="blogBlock">
    <div class="blogs">
        <% articals.forEach((article) => { %>
          <div class="blog">
            <div class="nameSection">
              <h3><a href="/startup/<%= article._id %>"><%= article.startupname %></a></h3>
              <p><%= article.ownername %></p>
              <p><%= article.domain %></p>
            </div>
            <div class="descriptionSection"><%= article.discription %></div>
            <div class="imageBlock"><img src="https://www.intelligenthq.com/wp-content/uploads/2018/07/medical-startups.jpg" alt=""></div>
          </div>
        <% }) %>
    </div>
   </div>
</body>
